{
    "configurations": [
        {
            "name": "Python: 当前文件",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "python": "${command:python.interpreterPath}",
            "pythonArgs": ["-Wa"],
            "console": "integratedTerminal",
            "justMyCode": false
        },
        {
            "name": "init_emqx",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "python": "${command:python.interpreterPath}",
            "pythonArgs": ["-Wa"],
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": [
                "f83hfldfjlkekfn34",
            ],
        },
        {
            "name": "runserver",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}\\manage.py",
            "python": "${command:python.interpreterPath}",
            "pythonArgs": ["-Wa"],
            "args": [
                "runserver",
                "0.0.0.0:8000",
                // "--noreload",
            ],
            "justMyCode": false,
            "django": true
        },
        {
            "name": "collectstatic",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}\\manage.py",
            "python": "${command:python.interpreterPath}",
            "pythonArgs": ["-Wa"],
            "args": [
                "collectstatic",
                "--noinput"
            ],
            "justMyCode": false,
            "django": true
        },
        {
            "name": "uvicorn",
            "type": "debugpy",
            "request": "launch",
            "module": "uvicorn",
            "python": "${command:python.interpreterPath}",
            "pythonArgs": ["-Wa"],
            "args": [
                "config.asgi:application",
                "--host", "0.0.0.0",
                "--port", "8000",
                "--workers", "1",
                "--log-level", "debug",
                "--reload"
            ],
            "justMyCode": false,
            "django": true
        },
        {
            "name": "makemigrations",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}\\manage.py",
            "python": "${command:python.interpreterPath}",
            "pythonArgs": ["-Wa"],
            "args": [
                "makemigrations",
            ],
            "justMyCode": false,
            "django": true
        },
        {
            "name": "migrate",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}\\manage.py",
            "python": "${command:python.interpreterPath}",
            "pythonArgs": ["-Wa"],
            "args": [
                "migrate",
            ],
            "justMyCode": false,
            "django": true
        }
    ]
}
