{
  "version": 1,
  "formatters": {
    "default": {
      "format": "%(asctime)s | %(levelname)s | %(name)s | %(message)s"
    }
  },
  "handlers": {
    "default": {
      "class": "logging.StreamHandler",
      "formatter": "default",
      "stream": "ext://sys.stdout"
    }
  },
  "loggers": {
    "uvicorn": {
      "handlers": ["default"],
      "level": "INFO"
    },
    "uvicorn.error": {
      "level": "INFO"
    },
    "uvicorn.access": {
      "level": "INFO"
    }
  },
  "root": {
    "handlers": ["default"],
    "level": "INFO"
  }
}
